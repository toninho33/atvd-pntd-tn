programa {
  funcao inicio() {
    inteiro valorA, valorB, valorC
    real soma, multiplicacao

    escreva("Digite o valor A: ")
    leia(valorA)

    escreva("Digite o valor B: ")
    leia(valorB)

    se(valorA == valorB) 
    soma = valorA + valorB
    escreva("A soma é ", soma)

   se(valorA != valorB) 
   multiplicacao = valorA * valorB
   escreva("A multiplicacao é ", multiplicacao)








  }
}
